{
  "name": "@hyj/react-formctl",
  "version": "0.0.0",
  "description": "A monorepo for my packages",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cicada1992/react-formctl.git"
  },
  "license": "ISC",
  "scripts": {
    "start:demo:form": "pnpm -F '@demo/react-formctl' dev",
    "build:pack:form": "pnpm -F '@hyj/react-formctl' build",
    "publish:pack:form": "pnpm -F '@hyj/react-formctl' publish --access=public",
    "flush": "sh flush.sh",
    "copy-readme": "sh copy-readme.sh",
    "prettier": "prettier --write '**/*.{ts,js,jsx,tsx}'"
  },
  "devDependencies": {
    "prettier": "^3.3.2",
    "typescript": "^5.5.2"
  }
}